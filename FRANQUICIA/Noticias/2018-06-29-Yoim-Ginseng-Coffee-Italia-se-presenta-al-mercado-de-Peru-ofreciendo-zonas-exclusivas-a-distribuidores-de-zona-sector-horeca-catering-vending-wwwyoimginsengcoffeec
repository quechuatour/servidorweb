	    <script type="text/javascript">
function deseleccionar_todo(){
    document.getElementById('desmarcar2').style.display = "none";
    document.getElementById('marcar2').style.display = "block";
    document.getElementById('desmarcar').style.display = "none";
    document.getElementById('marcar').style.display = "block";
   for (i=0;i<document.ficha.elements.length-3;i++)
   {
	if(document.ficha.elements[i].type == "checkbox")
	{
		document.ficha.elements[i].checked=0
	}
    }
} 
function seleccionar_todo(){
    document.getElementById('desmarcar2').style.display = "block";
    document.getElementById('marcar2').style.display = "none";
    document.getElementById('desmarcar').style.display = "block";
    document.getElementById('marcar').style.display = "none";
   for (i=0;i<document.ficha.elements.length-3;i++)
   {
	if(document.ficha.elements[i].type == "checkbox")
	{
		document.ficha.elements[i].checked=1
	}
    }
} 
</script>

